<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SignUpController">
   <children>
      <VBox layoutX="278.0" layoutY="133.0" prefHeight="131.0" prefWidth="245.0" spacing="20.0">
         <children>
            <TextField fx:id="signUpUserName" promptText="user name" />
            <TextField fx:id="signUpEmail" prefHeight="10.0" prefWidth="245.0" promptText="email" />
            <TextField fx:id="signUpPassword" promptText="password" />
         </children>
      </VBox>
      <VBox layoutX="357.0" layoutY="296.0" prefHeight="77.0" prefWidth="100.0" spacing="20.0">
         <children>
            <Button mnemonicParsing="false" onAction="#signUp" prefHeight="27.0" prefWidth="75.0" style="-fx-background-color: rgba(19, 173, 243, 0.63);" text="Sign Up" textFill="WHITE" />
            <Button mnemonicParsing="false" onAction="#login" prefHeight="27.0" prefWidth="75.0" style="-fx-background-color: rgba(19, 173, 243, 0.63);" text="Log In" textFill="WHITE" />
         </children>
      </VBox>
   </children>
</AnchorPane>
